        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Save-Script</source>
          <target state="translated">Save-Script</target>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Save-Script cmdlet lets you to review the script file by saving it to a specified location.</source>
          <target state="translated">Mit dem Cmdlet „Save-Script“ können Sie die Skriptdatei überprüfen, indem Sie sie an einem angegebenen Speicherort speichern.</target>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Description</source>
          <target state="translated">Beschreibung</target>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The Save-Script cmdlet saves the specified script.</source>
          <target state="translated">Das Save-Skript-Cmdlet wird das angegebene Skript gespeichert.</target>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Cmdlet syntax</source>
          <target state="translated">Cmdlet-syntax</target>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Cmdlet online help reference</source>
